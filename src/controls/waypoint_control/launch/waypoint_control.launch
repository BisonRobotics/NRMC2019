<launch>
    <node name="waypoint_control_server" pkg="waypoint_control" type="waypoint_control_server" output="screen">
        <param name="rate"              type="double" value="50" />
        <param name="max_acceleration"  type="double" value="1.00" />
        <param name="max_duty"          type="double" value="0.30" />
        <param name="max_in_place_duty" type="double" value="0.30" />
        <param name="min_in_place_duty" type="double" value="0.05" />
        <param name="max_driving_duty"  type="double" value="0.30" />
        <param name="min_driving_duty"  type="double" value="0.05" />
        <param name="max_manual_duty"   type="double" value="0.30" />
        <param name="min_manual_duty"   type="double" value="0.05" />
        <param name="in_place_k"        type="double" value="0.40" />
        <param name="driving_k"         type="double" value="0.10" />
    </node>
</launch>